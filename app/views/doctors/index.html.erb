<div class="jumbotron">
  <div class="container">
  <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a></p>
  </div>
</div>
  <h1>Listing Doctors</h1>
  <p id="notice"><%= notice %></p>
<% @doctors.each_slice(3) do |doctor| %>
<div class="row">
  <% doctor.each do |doctors| %>
  <div class="col-md-4">
    
    <div class="thumbnail">
     <%= image_tag doctors.image.url , class:"img-thumbnail"%>
      <div class="caption">
        <h3><%= doctors.name %></h3>
        <td><%= doctors.website %></td>
        <td><%= doctors.phone %></td>
        <td><%= doctors.title %></td>
        <td><%= link_to 'Show', doctors %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctors, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <p><%= doctors.title %></p>
        <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
        
    
      </div>

    </div>

  </div>
 <% end %>
</div>
 <% end %>








